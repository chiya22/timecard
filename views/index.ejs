<!DOCTYPE html>
<html>

<head>
  <title><%= title %></title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <div class="container-top">
    <div class="header">
      <h1><%= title %></h1>
    </div>
    <div class="area">
      <h2>社員</h2>
      <div class="items">
        <% userlist.forEach(function(user){ %>
        <a href="time/<%= user.id %>"><%= user.name %></a>
        <% }) %>
      </div>
    </div>
    <div class="area">
      <h2>アルバイト</h2>
      <div class="items">
        <% parttimeuserlist.forEach(function(user){ %>
          <a href="time/<%= user.id %>"><%= user.name %></a>
          <% }) %>
        </div>
    </div>
  </div>

</body>

</html>