<!DOCTYPE html>
<html>

<head>
  <title><%= title %></title>
  <%- include("./common/stylesheets.ejs", {isAdmin: true}) %>
</head>

<body>
  <%- include("./common/header.ejs", {adminMenu: false, backMenu: false, backAdmin: true, backToMonth: false, id: null}) %>
  <div class="container-top">
    <div class="area">
      <% if (yyyymmlist) { %>
          <div class="items mt-30">
            <% yyyymmlist.forEach(function(yyyymm){ %>
              <a class="wow fadeIn" href="<%= user.id %>/<%= yyyymm.yyyymm_seisan.slice(0,4) + yyyymm.yyyymm_seisan.slice(-2) %>"><%= yyyymm.yyyymm_seisan.slice(0,4) + '/' + yyyymm.yyyymm_seisan.slice(-2) %></a>
            <% }) %>
          </div>
      <% } %>
    </div>
  </div>
</body>
<%- include("./common/javascripts.ejs") %>

</html>